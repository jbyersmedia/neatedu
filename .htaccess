RewriteEngine On
#RewriteCond %{HTTP_HOST} ^neatedu\.net [NC]
#RewriteRule (.*) http://www.neatedu.net/$1 [R=303,L]

RewriteCond %{HTTP_HOST}                !^$
RewriteCond %{HTTP_HOST}                !^www\.neatedu\.net$ [NC]
RewriteCond %{HTTP_HOST}                !^casite\-638521\.cloudaccess\.net$ [NC] // live site
RewriteCond %{HTTP_HOST}                !^casite\-1056849\.cloudaccess\.net$ [NC] // stage site
RewriteRule ^/?(.*)                     https://www.neatedu.net/$1 [R=303,L,NC]

RewriteCond %{SERVER_PORT} 80
RewriteRule ^/?(.*)                     https://%{HTTP_HOST}/$1 [R=303,L,NC]

RedirectMatch	permanent	^/$	/a/schedule
RedirectMatch   permanent       ^/index.html	/a/schedule
RedirectMatch   permanent       ^/index.php	/a/schedule
